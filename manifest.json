{
  "manifest_version": 3,
  "name": "Expedia Price Scraper",
  "version": "3.1",
  "permissions": ["identity", "tabs", "scripting", "storage", "alarms"],
  "host_permissions": ["https://www.expedia.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "key": ""
}
